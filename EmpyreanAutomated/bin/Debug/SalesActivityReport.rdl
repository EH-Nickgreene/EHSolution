  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EHClinical</DataSourceName>
        <CommandText>
DECLARE @StartDate date,@EndDate date

SET @StartDate =
(
	SELECT MIN(Date)
	FROM (
		SELECT TOP 7 date
		FROM Reference.dbo.DimDate
		WHERE date&lt;getdate()
		and day='Sunday'
		ORDER BY date desc
		) a
)


SET @EndDate= dateadd(dd,43,@StartDate)

--FRIST QUERY CREATE BASE SO A RECORD IS RETURNED FOR EVERY COMBINATION
--0 FOR TOTAL GUARANTEES IT WONT SKEW ANY TOTALS
SELECT 
	metric.Metric MetricName,
	CASE
		WHEN metric.Metric='CallTracking' THEN 1
		WHEN metric.Metric='LeadTracking' THEN 2
		WHEN metric.Metric='Admissions' THEN 3
		ELSE 4
	END as MetricOrder,
	sth.ExcelRepName SalesRep,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate,
	0 as Total
FROM [dbo].[SalesTargets] st
INNER JOIN HCHB_EMPYREAN.dbo.ADMISSION_COORDINATORS ac on ac_id=st.AdmissionCoordinatorID and ac.ac_active='Y'
LEFT JOIN SalesExcelRepNamesToHCHBRepNames sth on sth.HCHBRepName=ac_lastname+', '+ac_Firstname
CROSS JOIN (
		SELECT
		DISTINCT WeeKofYearDisplay,WeekStartDate
		FROM Reference.dbo.dimdate dd 
		WHERE dd.date&gt;=@StartDate
		and dd.date&lt;@EndDate
	) dd 
CROSS JOIN (
	SELECT 'CallTracking' Metric
	UNION ALL
	SELECT 'LeadTracking'
	UNION ALL
	SELECT 'Admissions'
	)	Metric

UNION ALL



SELECT 
	'CallTracking' MetricName,
	1 as MetricOrder,
	a.SalesRep,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate,
	COUNT(*) Total
	--SELECT * 
FROM EHClinical.dbo.SalesCallTracking a
LEFT JOIN Reference.dbo.dimdate dd on a.ActivityDate=dd.date
WHERE dd.date&gt;=@StartDate
and dd.date&lt;@EndDate
AND (CASE WHEN ISNULL(Location,'') &lt;&gt;'' THEN 1 ELSE 0 END+
CASE WHEN ISNULL(ReferralSource,'') &lt;&gt;'' THEN 1 ELSE 0 END+
CASE WHEN ISNULL(ContactType,'') &lt;&gt;'' THEN 1 ELSE 0 END+
CASE WHEN ISNULL(ContactName,'') &lt;&gt;'' THEN 1 ELSE 0 END+
CASE WHEN ISNULL(ContactRole,'') &lt;&gt;'' THEN 1 ELSE 0 END+
CASE WHEN ISNULL(Notes,'') &lt;&gt;'' THEN 1 ELSE 0 END)&gt;=3
GROUP By a.SalesRep,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate
UNION ALL 
SELECT 
	'LeadTracking' MetricName,
	2 as MetricOrder,
	a.SalesRep,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate,
	COUNT(*)
FROM EHClinical.dbo.SalesLeadTracking a
LEFT JOIN Reference.dbo.dimdate dd on a.ActivityDate=dd.date
WHERE dd.date&gt;=@StartDate
and dd.date&lt;@EndDate
GROUP By a.SalesRep,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate
UNION ALL 
SELECT
	'Admissions' MetricName,
	3 as MetricOrder,
	sth.ExcelRepName,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate,
	COUNT(*)
FROM wh_admissions a
LEFT JOIN SalesExcelRepNamesToHCHBRepNames sth on sth.HCHBRepName=a.AdmissionCoord
LEFT JOIN Reference.dbo.dimdate dd on a.epi_socdate=dd.date
WHERE dd.date&gt;=@StartDate
and dd.date&lt;@EndDate
GROUP BY 
	sth.ExcelRepName,
	dd.WeeKofYearDisplay,
	dd.WeekStartDate</CommandText>
      </Query>
