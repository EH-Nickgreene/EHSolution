    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EHClinical</DataSourceName>
        <CommandText>
DECLARE @monthInt INT

SELECT @monthInt =202403

DECLARE @StartDate date,@EndDate date

SELECT @StartDate=MIN(date)
FROM Reference.dbo.DimDate
WHERE monthint=@monthInt

IF GETDATE()&gt;(SELECT MAX(date) 
				FROM Reference.dbo.DimDate
				WHERE monthint=@monthInt)
BEGIN
	SELECT @EndDate=dateadd(dd,1,MAX(date))
	FROM Reference.dbo.DimDate
	WHERE monthint=@monthInt
END
ELSE
BEGIN
	set @EndDate=CONVERT(date,getdate())
END




IF OBJECT_ID('tempdb..#admits') IS NOT NULL DROP TABLE #admits
CREATE TABLE #admits(BranchCode varchar(10),BranchName varchar(200),epi_socdate date, DayOfMonth INT,Admits float)
INSERT INTO #admits
exec usp_forecastadmissions @StartDate,@EndDate


IF OBJECT_ID('tempdb..#Discharges') IS NOT NULL DROP TABLE #Discharges
CREATE TABLE #Discharges(BranchCode varchar(10),BranchName varchar(200),epi_dischargeDate date, DayOfMonth INT,Discharges float)
INSERT INTO #Discharges
exec usp_forecastdischarges @StartDate,@EndDate

--SELECT * FROM #Discharges
--SELECT * FROM #admits

IF OBJECT_ID('tempdb..#census') IS NOT NULL DROP TABLE #census
SELECT 
	bd.branchCode,
	'2024-3-1' ServiceDate,
	CASE
		WHEN BranchCode='EAD' THEN 2 
		WHEN BranchCode='EAI' THEN  46
		WHEN BranchCode='EAT' THEN 25
		WHEN BranchCode='EAU' THEN  10
		WHEN BranchCode='ECL' THEN 23
		WHEN BranchCode='ECO' THEN 1
		WHEN BranchCode='ENW' THEN 9
		ELSE 0
	END Census
	--COUNT(*) Census
INTO #census
FROM wh_PatientDays wh
LEFT JOIN Reference.dbo.BranchData bd on wh.Branch_Code=bd.BranchCode
WHERE wh.serviceDate&gt;='2024-2-22'
and wh.serviceDate&lt;'2024-2-23'
GROUP BY bd.branchCode

	
	--SELECT * FROM #census
	--SELECT * FROM #ReadyForAgg
IF OBJECT_ID('tempdb..#ReadyForAgg') IS NOT NULL DROP TABLE #ReadyForAgg
SELECT 
	CASE
		WHEN c.census IS NOT NULL
		THEN 'actual'
		ELSE 'forecast'
	END rowtype,
	admit.BranchCode,
	admit.BranchName,
	admit.epi_socdate date,
	admit.DayOfMonth,
	admit.Admits,
	dc.Discharges,
	c.Census,
	LAG(dc.Discharges,1,0) OVER(PARTITION BY admit.BranchCode order by admit.epi_socdate) DcDayBefore
	--LAG(c.Census,1,0) OVER(PARTITION BY admit.BranchCode order by admit.epi_socdate) ForecastCensus

INTO #ReadyForAgg
FROM #admits admit
LEFT JOIN #Discharges dc ON admit.BranchCode=dc.BranchCode and admit.epi_socdate=dc.epi_dischargeDate
LEFT JOIN #census c on c.BranchCode=admit.BranchCode and c.ServiceDate=admit.epi_socdate
--WHERE admit.BranchCode='EAI'
ORDER BY 3


DECLARE @LastDayCensus float

SET @LastDayCensus =(SELECT TOP 1 Census FROM #ReadyForAgg WHERE Census IS NOT NULL ORDER BY date desc )

UPDATE ra
SET Census=(SELECT TOP 1 Census FROM #ReadyForAgg WHERE Census IS NOT NULL and BranchCode=ra.BranchCode ORDER BY date desc )
FROM #ReadyForAgg ra
WHERE Census IS NULL

SELECT 
ra.rowtype,
ra.BranchCode,
ra.BranchName,
ra.date,
ra.DayOfMonth,
ra.Admits,
ra.Discharges,
ra.Census,
CASE
	WHEN rowtype='actual' THEN census
	ELSE census+Admitforecast-Dischargeforecast
END projectedCensus

	
FROM #ReadyForAgg ra
OUTER APPLY (
	SELECT SUM(Admits) Admitforecast
	FROM #ReadyForAgg 
	WHERE BranchCode=ra.BranchCode
	and date&lt;=ra.date
	and rowtype='forecast'
	) f_admit
OUTER APPLY (
	SELECT SUM(DcDayBefore) Dischargeforecast
	FROM #ReadyForAgg 
	WHERE BranchCode=ra.BranchCode
	and date&lt;=ra.date
	and rowtype='forecast'
	) f_discharge</CommandText>
