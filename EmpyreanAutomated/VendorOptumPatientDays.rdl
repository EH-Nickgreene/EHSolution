  <DataSets>
    <DataSet Name="PatientDays">
      <Query>
        <DataSourceName>EHClinical</DataSourceName>
        <CommandText>DECLARE @StartDate date,@EndDate date

SET @StartDate =DATEADD(mm,DATEDIFF(mm,0,GETDATE())-1,0)
SET @EndDate =  DATEADD(mm,DATEDIFF(mm,0,GETDATE()),0)



SELECT
wh.Branch_Code,b.Branch_Name,COUNT(*) PatientDays
FROM wh_patientDays wh
LEFT JOIN HCHB_EMPYREAN.dbo.Branches b on wh.branch_code=b.branch_code
WHERE ServiceDate&gt;=@StartDate
and ServiceDate&lt;@EndDate
GROUP BY 
	wh.Branch_Code,
	b.Branch_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Branch_Code">
          <DataField>Branch_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Branch_Name">
          <DataField>Branch_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PatientDays">
          <DataField>PatientDays</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
