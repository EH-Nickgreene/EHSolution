    <DataSet Name="LeadsByReferralSource">
      <Query>
        <DataSourceName>EHClinical</DataSourceName>
        <CommandText>
SELECT 
	a.SalesRep,
	CASE
		WHEN a.ReferralSource IN ('FA- *NONE::1001','zz- Not Available') THEN 'Unknown'
		ELSE COALESCE(fa_name,ph_lastname+', ' +ph_firstname,'Unknown')
	END ReferralSource,
	COUNT(*) Leads,
	ROW_NUMBER() OVER(PARTITION BY a.SalesRep ORDER BY COUNT(*) desc) RankID
--SELECT * 
FROM EHClinical.dbo.SalesLeadTracking a
LEFT JOIN HCHB_Empyrean.dbo.Facilities fa on LEFT(a.ReferralSource,2)='FA' AND 
										 	fa.fa_id=CASE 
												WHEN ReferralSource LIKE '%::%' THEN SUBSTRING(ReferralSource,CHARINDEX('::',ReferralSource)+2,100)
												ELSE ''
											END 
LEFT JOIN HCHB_Empyrean.dbo.Physicians ph on LEFT(a.ReferralSource,2)='PH' AND 
											ph.ph_id=CASE 
												WHEN ReferralSource LIKE '%::%' THEN SUBSTRING(ReferralSource,CHARINDEX('::',ReferralSource)+2,100)
												ELSE ''
											END 

GROUP BY 
	a.SalesRep,
	CASE
		WHEN a.ReferralSource IN ('FA- *NONE::1001','zz- Not Available') THEN 'Unknown'
		ELSE COALESCE(fa_name,ph_lastname+', ' +ph_firstname,'Unknown')
	END
</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesRep">
          <DataField>SalesRep</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReferralSource">
