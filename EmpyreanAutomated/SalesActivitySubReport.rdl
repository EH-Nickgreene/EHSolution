    <DataSet Name="AdmitsByReferralSource">
      <Query>
        <DataSourceName>EHClinical</DataSourceName>
        <CommandText>
--TOP Admissions BY referralSoruce
SELECT 
	sth.ExcelRepName SalesRep,
	a.ReferralName,
	COUNT(*) Admits,
	SUM(whp.LOS) as TotalLOS,
	ROW_NUMBER() OVER(PARTITION BY sth.ExcelRepName ORDER BY COUNT(*) desc,SUM(whp.LOS) desc) RankID
FROM wh_admissions a
LEFT JOIN SalesExcelRepNamesToHCHBRepNames sth on sth.HCHBRepName=a.AdmissionCoord
LEFT JOIN Reference.dbo.dimdate dd on a.epi_socdate=dd.date
LEFT JOIN (
		SELECT epi_paid,epi_socdate,COUNT(*) LOS
		FROM wh_PatientDays 
		WHERE serviceDate&gt;=getdate()-180
		GROUP BY epi_paid,epi_socdate
		) whp on whp.epi_paid=a.epi_paid and whp.epi_socdate=a.epi_socdate
WHERE sth.ExcelRepName IS NOT NULL
AND a.epi_socdate&gt;=GETDATE()-180
GROUP BY sth.ExcelRepName ,a.ReferralName
ORDER BY sth.ExcelRepName,COUNT(*) desc,SUM(whp.LOS) desc
</CommandText>
      </Query>
