  <DataSets>
    <DataSet Name="CancellationData">
      <Query>
        <DataSourceName>CentralReach</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LocationName">
            <Value>=Parameters!LocationName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeeksBack">
            <Value>=Parameters!WeeksBack.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT *,CONVERT(date,LEFT(WeekOfyear,10)) WeekStartDate FROM CancelratesbyBranch
WHERE Location=@LocationName
and WeeksBack&lt;=@WeeksBack</CommandText>
      </Query>
      <Fields>
        <Field Name="WeekOfYear">
          <DataField>WeekOfYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location">
